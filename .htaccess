RewriteEngine On
#RewriteBase /


RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# RewriteCond %{REQUEST_FILENAME}.php -f
RewriteCond %{DOCUMENT_ROOT}/htdocs/%{REQUEST_FILENAME}.php -f
RewriteRule ^(.+)$ /$1.php [L,QSA]


